{
  "name": "react-vote-example",
  "version": "1.0.0",
  "description": "React Example: Online Voting",
  "repository": {
    "type": "git",
    "url": "https://github.com/nilshartmann/react-vote-example.git"
  },
  "scripts": {
    "start": "npm run build && npm run server",
    "clean": "rimraf ./public/dist",
    "build": "npm run clean && webpack --progress --config webpack.config.dev.js",
    "server": "node ./src/server",
    "server:hot": "npm run clean && USE_HOTSERVER=true npm run server",
    "test": "mocha ./test --compilers js:babel-register --recursive",
    "test:integration": "karma start --single-run",
    "test:cov": "babel-node node_modules/.bin/isparta cover --dir ./_coverage node_modules/.bin/_mocha -- ./test --recursive",
    "import-mongodb-data": "mongoimport --db vote-db --collection votemodels ./src/server/db/mongodb-votes.json"
  },
  "author": "Nils Hartmann, Olli Zeigermann",
  "license": "MIT",
  "keywords": [
    "react",
    "redux",
    "react-router",
    "universal webapp"
  ],
  "devDependencies": {
    "babel-cli": "^6.4.0",
    "babel-core": "^6.4.0",
    "babel-eslint": "^5.0.0-beta6",
    "babel-loader": "^6.2.1",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.1",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.4.3",
    "css-loader": "0.23.1",
    "eslint": "^1.10.3",
    "eslint-loader": "^1.2.0",
    "eslint-plugin-react": "^3.14.0",
    "eventsource-polyfill": "^0.9.6",
    "expect": "1.13.0",
    "file-loader": "0.8.5",
    "hapi-webpack-plugin": "^1.3.0",
    "isparta": "4.0.0",
    "istanbul": "^0.4.1",
    "jsdom": "^7.1.0",
    "json-loader": "^0.5.3",
    "karma": "^0.13.19",
    "karma-chrome-launcher": "^0.2.1",
    "karma-mocha": "^0.2.1",
    "karma-mocha-reporter": "^1.1.3",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.4",
    "mocha-jsdom": "^1.0.0",
    "phantomjs": "^1.9.19",
    "react-addons-test-utils": "^0.14.0",
    "react-hot-loader": "^1.3.0",
    "redux-devtools": "^2.1.2",
    "rimraf": "^2.5.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.10",
    "webpack-dev-middleware": "^1.4.0",
    "webpack-hot-middleware": "^2.6.0"
  },
  "dependencies": {
    "hapi": "12.1.0",
    "history": "^1.17.0",
    "inert": "3.2.0",
    "isomorphic-fetch": "2.1.1",
    "mongoose": "^4.1.1",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-redux": "^4.0.0",
    "react-router": "^1.0.0",
    "redux": "^3.0.2",
    "react-router-redux": "2.1.0",
    "redux-thunk": "^1.0.0"
  }
}
